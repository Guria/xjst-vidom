{
  "name": "xjst-ddsl",
  "version": "1.0.1",
  "description": "Experimental ddsl engine for virtual dom libraries on xjst",
  "main": "index.js",
  "scripts": {
    "clean": "rm -f lib/ddsl/bundle.js",
    "make": "browserify --standalone ddsl lib/ddsl/index.js -o lib/ddsl/bundle.js",
    "postinstall": "npm run make",
    "test": "mocha --reporter=spec test/*-test.js"
  },
  "keywords": [
    "xjst",
    "vidom",
    "virtual",
    "dom",
    "react",
    "templates"
  ],
  "author": "Anton Winogradov <winogradovaa@gmail.com>",
  "dependencies": {
    "bem-xjst": "github:awinogradov/bem-xjst#js-bindings",
    "inherits": "^2.0.1"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "browserify": "^13.0.1",
    "camel-case": "^3.0.0",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "license": "MIT"
}
